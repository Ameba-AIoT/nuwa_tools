{
    "info": {
        "brief": "Realtek Nuwa Zephyr SDK",
        "soc": "Ameba",
        "os": "Zephyr",
        "vcs": "west",
        "meta": "nuwa.py",
	"debuggerArgs": null,
        "overrideLaunchCommands": [
            "mem 0xaaaaaaaa 0xaaaaaaab none",
            "set mem inaccessible-by-default off",
            "monitor reset 0",
            "monitor sleep 20",
            "monitor reset 0",
            "monitor sleep 20",
            "flushregs"
        ]
    },
    "apps": {
        "include": [
            "applications",
            "zephyr/samples"
        ],
        "exclude": [
            "zephyr/samples/arch/smp",
            "zephyr/samples/boards",
            "zephyr/samples/drivers/ethernet",
            "zephyr/samples/drivers/fpga",
            "zephyr/samples/drivers/ht16k33",
            "zephyr/samples/drivers/ipm",
            "zephyr/samples/drivers/mbox",
            "zephyr/samples/drivers/ps2",
            "zephyr/samples/drivers/soc_flash_nrf",
            "zephyr/samples/fuel_gauge",
            "zephyr/samples/sensor",
            "zephyr/samples/shields",
            "zephyr/samples/subsys/ipc"
        ]
    },
    "devices": {
        "rtl8721f_evb": {
            "chip": "AmebaG2",
            "path": "zephyr/boards/arm/rtl8721f_evb"
        },
            "rtl872xda_evb": {
            "chip": "AmebaDplus",
            "path": "zephyr/boards/arm/rtl872xda_evb"
        },
        "rtl872xd_evb": {
            "chip": "AmebaD",
            "path": "zephyr/boards/arm/rtl872xd_evb"
        }
    },
    "chips": {
        "AmebaG2": {
            "core": "Cortex-M33",
            "ToolChainVerMajor": "asdk-12.3.1",
            "ToolChainVerMinor": "4568",
            "TOOLCHAINURL": "https://github.com/Ameba-AIoT/ameba-toolchain/releases/download/12.3.1_v3/",
            "TOOLCHAINNAME": "-x86_64",
            "jlinkScript": "AmebaG2/AP2_AP.JLinkScript"
        },
        "AmebaDplus": {
            "core": "Cortex-M33",
            "ToolChainVerMajor": "asdk-10.3.1",
            "ToolChainVerMinor": "4523",
            "TOOLCHAINURL": "https://github.com/Ameba-AIoT/ameba-toolchain/releases/download/10.3.1_v6/",
            "TOOLCHAINNAME": "-x86_64_with_small_reent",
            "jlinkScript": "amebadplus/AP2_KM4.JLinkScript"
        },
        "AmebaD": {
            "core": "Cortex-M33",
            "ToolChainVerMajor": "asdk-10.3.1",
            "ToolChainVerMinor": "4523",
            "TOOLCHAINURL": "https://github.com/Ameba-AIoT/ameba-toolchain/releases/download/10.3.1_v6/",
            "TOOLCHAINNAME": "-x86_64_with_small_reent",
            "jlinkScript": "amebad/AP2_KM4.JLinkScript"
        }
    },
    "compatibilityInfo": [
        {
            "extensionId": "realtek.real-connect",
            "minVersion": "1.0.0"
        }
    ]
}
